{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Tests",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "pnpm",
        "runtimeArgs": [
          "test:inspect",
          "--watch"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": [
            "<node_internals>/**",
            "**/extensions/**/*.js",
            "**/node_modules/**/*.js"
        ]
      }
    ]
  }